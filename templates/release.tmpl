<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<link rel="stylesheet" href="/common.css">
	<title>gorelease - {{.Name}}</title>
</head>
<body>
	<div id="page">
		<h1>{{.Name}} <small>{{.Branch}}</small></h1>
		<table>
			<tr>
				<th>GOOS</th>
				<th>GOARCH</th>
				<th>Link</th>
				<th>Op.</th>
			</tr>
			{{range .Releases}}
			<tr>
				<td>{{.OS}}</td>
				<td>{{.Arch}}</td>
				<td>{{.Link}}</td>
				<td>
					<a href="{{.Link}}">Download</a>
				</td>
			</tr>
			{{end}}
		</table>
	</div>
	{{template "analysis" .}}
</body>
</html>
