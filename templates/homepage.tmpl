{[template "base/header" .]}
<script src="/js/homepage.js"></script>
<body>
	{[template "base/nav" .]}
	<div class="container" ng-controller="MainCtrl">
		<div class="row">
			<div class="col-md-12">
				<p>Golang cross build your code in github and publish it.</p>
				<p>use <a href="https://github.com/gorelease/gopack">gopack</a> to perform golang build</p>
			</div>
			<div class="col-md-12">
				<h4>Recent builds</h4>
				<table class="table">
					<tr>
						<th>Name</th>
						<th>Time</th>
					</tr>
					<tr ng-repeat='x in repos'>
						<td>
							<a href="/{{x.owner}}/{{x.repo}}">{{x.owner}}/{{x.repo}}</a>
							<small>master</small>
						</td>
						<td>{{x.trigger_at | fromNow }}</td>
					</tr>
				</table>
			</div>
			<hr>
			{[template "footer" .]}
			{[template "base/analysis" .]}
		<div>
	</div>
</body>
</html>
